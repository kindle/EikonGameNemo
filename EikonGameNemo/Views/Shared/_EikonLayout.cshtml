<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Eikon Game Nemo</title>
    <link href="~/Content/Eikon/schema2.css" rel="stylesheet" />
    <link href="~/Content/Eikon/main.css" rel="stylesheet" />
    <link href="~/Content/angular-motion.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-additions.min.css" rel="stylesheet" />
    @Styles.Render("~/bundles/site")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div style="width:99%;">
        @RenderBody()
    </div>
    @Scripts.Render("~/bundles/jquery")
    <script src="~/Scripts/ngmodules/angular-file-upload/angular-file-upload-shim.min.js"></script>
    <script src="~/Scripts/angular/angular.min.js"></script>
    <script src="~/Scripts/angular/angular-animate.min.js"></script>
    <script src="~/Scripts/angular/angular-sanitize.min.js"></script>
    <script src="~/Scripts/ngmodules/angular-translate/angular-translate.min.js"></script>
    <script src="~/Scripts/ngmodules/angular-file-upload/angular-file-upload.min.js"></script>
    <script src="~/Scripts/ngmodules/ui-bootstrap/ui-bootstrap-tpls-0.11.0.min.js"></script>
    <script src="~/Scripts/ngmodules/ui-router/angular-ui-router.min.js"></script>
    <script src="~/Scripts/ngmodules/ng-grid/ng-grid.min.js"></script>
    <script src="~/Scripts/ngmodules/ngStorage/ngStorage.min.js"></script>
    <script src="~/Scripts/ngmodules/angular-dialog-service/dialogs.min.js"></script>
    @Scripts.Render("~/bundles/app")
    @RenderSection("scripts", required: false)
</body>
</html>
